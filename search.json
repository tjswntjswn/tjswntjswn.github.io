---
layout: null
---
[
  {% for post in site.posts %}
    {
      "title"    : {{ post.title | jsonify }},
      "category" : {{ post.categories | join: ', ' | jsonify }},
      "tags"     : {{ post.tags | join: ', ' | jsonify }},
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date | date: "%Y.%m.%d" }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]